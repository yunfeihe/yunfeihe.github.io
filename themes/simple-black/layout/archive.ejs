<% page.posts.each(function (post) { %>
    <article class="post">
        <header class="entry-header">
            <h1 class="date"><%= date(post.date, "YYYY-MM") %></h1>

            <h2 class="entry-title">
                <a href="<%- url_for(post.path) %>"><%- post.title %></a>
            </h2>
        </header>
    </article>
<% }) %>

